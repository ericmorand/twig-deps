{
  "name": "twig-deps",
  "version": "1.0.3",
  "description": "Walk the dependency graph of a Twig template",
  "main": "src/index.js",
  "scripts": {
    "pretest": "npm prune && npm update",
    "test": "tap test/*.js --cov --reporter=spec --100",
    "testhtml": "tap test/*.js --cov --reporter=spec --100 --coverage-report=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericmorand/twig-deps.git"
  },
  "author": "Eric MORAND",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ericmorand/twig-deps/issues"
  },
  "homepage": "https://github.com/ericmorand/twig-deps#readme",
  "devDependencies": {
    "tap": "^10.3.0"
  },
  "dependencies": {
    "promise": "^7.1.1",
    "twig": "^1.10.4"
  }
}
